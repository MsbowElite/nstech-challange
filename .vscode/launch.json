{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker: Attach to .NET Container",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickRemoteProcess}",
      "pipeTransport": {
        "pipeCwd": "${workspaceRoot}",
        "pipeProgram": "docker",
        "pipeArgs": [
          "exec",
          "-i",
          "orderservice_api"
        ],
        "debuggerPath": "/vsdbg/vsdbg",
        "quoteArgs": false
      },
      "sourceFileMap": {
        "/src": "${workspaceRoot}/src",
        "/build/src": "${workspaceRoot}/src",
        "/app": "${workspaceRoot}/src"
      }
    }
  ]
}
